# AI Provider API Keys (choose one based on config.yaml ai_provider setting)

# OpenAI API Configuration (can be used with LiteLLM)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1  # Change to your LiteLLM endpoint URL

# Gemini API Configuration 
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Dataset Configuration
OUTPUT_DIR=./output
DATASET_SIZE=1000
BATCH_SIZE=10
